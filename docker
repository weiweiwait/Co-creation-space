docker run -it --name="kibana" -p 5602:5601 --link elasticsearch:elasticsearch kibana:7.10.1
sudo docker cp ./elasticsearch-analysis-ik-7.10.1.zip elasticsearch:/usr/share/elasticsearch/plugins/ik
